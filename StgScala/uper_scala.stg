octect_VarSize_decode(p, sAcc, nSizeMin, nSizeMax, nSizeInBits, sErrCode) ::= <<
// decode length
codec.decodeConstraintWholeNumber(<nSizeMin>, <nSizeMax>) match // uper:475
    case Some(n) => 
        assert(n >= 0)
        <p><sAcc>nCount = n
    case None() =>
        return Left(<sErrCode>)

