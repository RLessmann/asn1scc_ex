call_base_type_func_encode(p, sFuncName) ::= <<
<sFuncName>(<p>, codec, false) match // uper:6
    case Right(retVal) =>
        ret = Right(retVal)
    case Left(err) =>
        return Left(err)
>>
call_base_type_func_decode(p, sFuncName) ::= <<
<sFuncName>(codec) match // uper:13
    case Right(decData) => 
        <p> = decData
    case Left(err) => 
        return Left(err)
>>

