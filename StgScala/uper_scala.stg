codec.decodeOctetString_no_length(<p><sAcc>nCount.asInstanceOf[Int]) match // uper:483
    case SomeMut(a) =>    
        a.copyToArray(<p><sAcc>arr)
    case NoneMut() => 
        return Left(<sErrCode>)
>>

